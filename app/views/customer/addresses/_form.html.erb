<%= form_with model: model, url: url do |form| %>
  <div class="card card-primary">
    <div class="card-header">
      <h3 class="card-title">Indirizzo</h3>
    </div>
    <div class="card-body">
      <div class="form-group">
        <%= form.label :name %>
        <%= form.text_field :name, class: "form-control" %>
      </div>
      <div class="form-group">
        <%= form.label :country %>
        <%= form.text_field :country, class: "form-control" %>
      </div>
      <div class="form-group">
        <%= form.label :province %>
        <%= form.text_field :province, class: "form-control" %>
      </div>
      <div class="form-group">
        <%= form.label :city %>
        <%= form.text_field :city, class: "form-control" %>
      </div>
      <div class="form-group">
        <%= form.label :postcode %>
        <%= form.text_field :postcode, class: "form-control" %>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-12">
      <%= form.submit :class => "btn btn-success float-right" %>
    </div>
  </div>
<% end %>
